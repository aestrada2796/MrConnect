{
  "name": "aestrada2796/mrconnect",
  "type": "library",
  "description": "Integracíón con el sistema de API de MultiRumbo Corp",
  "keywords": [
    "MultiRumbo",
    "Multirumbo Corp",
    "Integration API"
  ],
  "homepage": "https://github.com/aestrada2796/mrconnect",
  "license": "MIT",
  "authors": [
    {
      "name": "Adrian Estrada",
      "email": "aestrada2796@gmail.com",
      "homepage": "https://www.adriandeveloper.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "laravel/framework": "^9.34"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "MrConnect\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MrConnect\\Tests\\": "tests/"
    }
  },
  "extra": {
    
    "laravel": {
      "providers": [
        "MrConnect\\MrConnectServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "psalm": "vendor/bin/psalm"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
